{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "Components/Individual/BusinessInfoForm.jsx",
    "Components/Individual/ContactPersonForm.jsx",
    "Components/Business/BusinessInfoFormForBusiness.jsx",
    "Components/Business/ContactPersonFormForBusiness.jsx",
    "app/signup/individual/page.jsx",
    "app/signup/business/page.jsx",
    "app/page.jsx",
    "Components/Footer.jsx",
    "Components/Header.jsx",
    "app/layout.jsx",
    "app/signup/page.jsx"
  ],
  "exclude": ["node_modules"]
}
